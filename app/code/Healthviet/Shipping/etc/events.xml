<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="sales_model_service_quote_submit_before">
        <observer name="healthviet_shipping_observer_sales_quotesubmitbefore"
                  instance="Healthviet\Shipping\Observer\Sales\QuoteSubmitBeforeObserver" />
    </event>
    <event name="checkout_submit_all_after">
        <observer name="healthviet_shipping_observer_checkout_submit_all_after"
                  instance="Healthviet\Shipping\Observer\Quote\CheckoutAllSubmitAfterObserver"/>
    </event>
    <event name="sales_quote_address_save_before">
        <observer name="healthviet_shipping_observer_sales_quote_address_save_before"
                  instance="Healthviet\Shipping\Observer\Sales\QuoteAddressSaveBeforeObserver" />
    </event>
</config>
